{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    // "source.fixAll.stylelint": true,
    "source.organizeImports": true
  },
  // "stylelint.ignoreDisables": true,
  // "stylelint.reportNeedlessDisables": true,
  "editor.snippetSuggestions": "top",
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "css.validate": false,
  "scss.validate": false,
  // "stylelint.validate": ["css", "scss", ".module.css", ".module.scss"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "vue.features.codeActions.enable": false,
  "cSpell.language": "en,es-ES",
  "cSpell.enabled": true,
  "cSpell.words": [
    "deseestructuraci√≥n",
    "lugamafe",
    "middlewares",
    "Parens",
    "refactorizar",
    "reutilizable",
    "settear",
    "volta"
  ],
  "todo-tree.highlights.defaultHighlight": {
    "icon": "alert",
    "type": "text-and-comment",
    "foreground": "black",
    "background": "white",
    "opacity": 50,
    "iconColour": "blue",
    "gutterIcon": true
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check",
      "foreground": "#2e2e2e",
      "background": "#fffffff1",
      "iconColour": "white"
    },
    "NOTE": {
      "icon": "note",
      "foreground": "white",
      "background": "gray",
      "iconColour": "gray"
    },
    "COMMENT": {
      "icon": "note",
      "foreground": "white",
      "background": "gray",
      "iconColour": "gray"
    },
    "FIXME": {
      "foreground": "black",
      "background": "yellow",
      "iconColour": "yellow"
    },
    "BUG": {
      "foreground": "black",
      "background": "red",
      "iconColour": "red"
    },
    "[ ]": {
      "icon": "check",
      "foreground": "black",
      "background": "white",
      "iconColour": "yellow"
    },
    "[x]": {
      "icon": "check",
      "foreground": "white",
      "background": "green",
      "iconColour": "green"
    }
  },
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "NOTE", "COMMENT", "[ ]", "[x]"],
  "todo-tree.regex.regex": "(?:(?://|#|<!--|;|/\\*\\*?|\\*)\\s*($TAGS)|^\\s*- \\[ \\])"
}
